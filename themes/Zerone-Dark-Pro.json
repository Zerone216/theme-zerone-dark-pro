	"tokenColors": [
		{
			"scope":"",
			"settings": {
				"background": "#ffffff",
				"foreground": "#000000"
			}
		},
		{
			"name": "Comment", //注释
			"scope": [
				"comment",
				"comment.line",
				"comment.block",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#56ff88"
			}
		},
		{
			"name": "Character",
			"scope": [
				"constant",
				"constant.character"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Class",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#25257c"
			}
		},
		
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#44a5ff"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5b5be9"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ff3737"
			}
		},
		{
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ff9d2d"
			}
		},
		{
			"name": "Parameter",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#1bffe1"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#1fa135"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#853da7"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6464f0"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"support.variable",
				"variable"
			],
			"settings": {
				"foreground": "#5affe9"
			}
		},
		///////////////////////////////////comment/////////////////////////////////
		{
			"scope":"comment", //注释
			"settings": {
				"fontStyle": "italic",
				"foreground": "#36fa78f1"
			}
		},
		{
			"scope": [
				"comment.line", //单行注释
				"comment.block",//多行注释块
				"comment.block.documentation", //用于文档的多行注释
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00fd54f1"
			}
		},
		///////////////////////////////////constant/////////////////////////////////
		{
			"scope":"constant", //常量
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fa3636f1"
			}
		},
		{
			"scope": [
				"constant.character" //字符常量
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff0000f1"
			}
		},
		{
			"scope": [
				"constant.character.escape", // 字符串中的字符转义符，例如\n和\x20
				"constant.numeric",          // 数字
				"emphasis"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fb621bf1"
			}
		},
		{
			"scope": [
				"constant.numeric"  //数字常量
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff0f0ff1"
			}
		},
		{
			"scope": [
				"constant.rgb-value" //RGB数值
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff3737"
			}
		},
		{
			"scope": [
				"constant.regexp" //
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff3737"
			}
		},
		{
			"scope": "constant.language", //语言中内置的常量，例如布尔值和空值
			"settings": {
				"foreground": "#f58f8f"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#c1e7ad"
			}
		},
		{
			"scope": [
				"constant.other" //
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff2f2f"
			}
		},
		{
			"scope": [
				"constant.other.placeholder" //格式化占位符，如那些用于printf()，例如%s
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#df2201"
			}
		},
		///////////////////////////////////entity/////////////////////////////////
		{
			"scope": "entity", //
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3b7ba7"
			}
		},
		{
			"scope": "entity.name", //将实体作用域分配给代码和标记中的数据结构，类型和其他唯一可识别的构造的名称
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2873a5"
			}
		},
		{
			"scope": [ //类型别名(alias)
				"entity.name.class",
				"entity.name.struct",
				"entity.name.enum",
				"entity.name.union",
				"entity.name.trait",
				"entity.name.interface",
				"entity.name.impl",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2497e4"
			}
		},
		{
			"scope": [
				"entity.name.class.forward-decl" //forward-decl上面的变体在诸如C和C ++的语言中使用
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#38aaf6"
			}
		},
		{
			"scope": [
				"entity.name.function", //函数
				"entity.name.function.constructor", //构造函数
				"entity.name.function.destructor" //析构函数
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1c73ad"
			}
		},
		{
			"scope": "entity.name.function.operator", //运算符重载
			"settings": {
				"foreground": "#adadad"
			}
		},
		{
			"scope": "entity.name.function.preprocessor", // 宏定义标签，包括函数和常量
			"settings": {
				"foreground": "#2bbcff"
			}
		},
		{
			"scope": [
				"entity.name.namespace" //命名空间，包和模块
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1d5e8a"
			}
		},
		{
			"scope": [
				"entity.name.constant" //常量,具体取决于语言语义。此范围通常包含在符号列表和索引中
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#337bac"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal",
			"settings": {
				"foreground": "#DADADA"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.number",
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.string",
			"settings": {
				"foreground": "#D69D85"
			}
		},
		{
			"scope": [
				"entity.name.label" //类似goto的构造的标签
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ec7e6a"

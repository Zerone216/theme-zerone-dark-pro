				"foreground": "#3cd2ff"
			}
		},
		{
			"scope": "entity.name",
			"settings": {
				"foreground": "#C8C8C8"
			}
		},
		{
			"scope": "keyword.control.directive",       /* 预处理关键字 */
			"settings": {
				"foreground": "#ffee06"
			}
		},
		{
			"scope": "variable",                         /* 变量 */
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "entity.name.function",             /* 函数名 */
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "entity.name.function.operator",
			"settings": {
				"foreground": "#B4B4B4"
			}
		},
		{
			"scope": "entity.name.function.preprocessor", /* 宏定义标签，包括函数和常量 */
			"settings": {
				"foreground": "#90dcff"
			}
		},
		{
			"scope": "entity.name.label",
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"scope": "entity.name.namespace",
			"settings": {
				"foreground": "#C8C8C8"
			}
		},
		{
			"scope": [
				"entity.name.type", /* 结构体别名（alias） */
				"entity.name.class"
			],
			"settings": {
				"foreground": "#25baff"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal",
			"settings": {
				"foreground": "#DADADA"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.number",
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.string",
			"settings": {
				"foreground": "#D69D85"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": "variable.other.enummember", /* eum 变量 */
			"settings": {
				"foreground": "#ff95ca"
			}
		},
		{
			"scope": "variable.other.property", /* 结构体成员引用 */
			"settings": {
				"foreground": "#22eb4d"
			}
		},
		{
			"scope": "variable.parameter", /* 函数形参 */
			"settings": {
				"foreground": "#ffffff"
			}
		}
	]
}
